<br><div class="container">

      <!-- Portfolio Section Heading -->
      <h2 class="page-section-heading text-center text-uppercase text-secondary mb-0">Games</h2>

      <!-- Icon Divider -->
      <div class="divider-custom">
        <div class="divider-custom-line"></div>
        <div class="divider-custom-icon">
          <i class="fas fa-gamepad"></i>
        </div>
        <div class="divider-custom-line"></div>
      </div><br><br><br>
    </div>

    <h3>Popular Games</h3>

    <div class="owl-carousel owl-theme">
    <div class="item"><img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQAbVSuky-RSqFamO1y1NZROOjAh9_vWSMcsFTHUa3jdoFbQQqx&s"><h6><center>Supermario Smash Brawl</center></h6></div>
    <div class="item"><img src="data:image/jpeg;base64,"><h6><center>Fifa 2019</center></h6></div>
    <div class="item"><img src="data:image/jpeg;base64,"><h6><center>Call of Duty MW2</center></h6></div>
</div>

<br><br><h3>All games</h3>  

<div class="row">
  <div class="card-deck">
    <div class="container-fluid content-row">
      <div class="row">
        <% @game.each_with_index do |game, index| %>

        <div class="col-sm-12 col-lg-6">
          <a href= "<%= "games/#{game.id}"%>" style="text-decoration: none">
            <div class="card mb-3">

                <h3 class="card-header"><%= game.game_name%> 
                </h3>
                  <div class="card-body">
              <%= game.platform %>    
               </div>
					
            </div>  
          </a>
        </div>
     <% end %></div>
    </div>
  </div>
</div>

